{
    "manifest_version": 3,
    "name": "PC to NAS File Transfer",
    "version": "1.0.0",
    "description": "A Chrome extension to transfer files from PC to NAS.",
    "permissions": [
        "storage",
        "contextMenus",
        "activeTab",
        "scripting"
    ],
     "host_permissions": [
        "http://192.168.5.2/*"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "48": "icons/icon48.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    }
}